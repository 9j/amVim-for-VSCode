{
	"name": "vim",
	"version": "0.0.1",
	"publisher": "auiWorks",
	"displayName": "Vim",
	"description": "Vim mode for Visual Studio Code.",
	"keywords": [
		"vim",
		"vintage"
	],
	"categories": [
		"Other"
	],
	"engines": {
		"vscode": "^0.10.1"
	},
	"main": "./out/src/extension",
	"contributes": {
		"keybindings": [
			{
				"command": "vim.h",
				"key": "h",
				"when": "editorTextFocus"
			},
			{
				"command": "vim.l",
				"key": "l",
				"when": "editorTextFocus"
			},
			{
				"command": "vim.j",
				"key": "j",
				"when": "editorTextFocus"
			},
			{
				"command": "vim.k",
				"key": "k",
				"when": "editorTextFocus"
			{
			}
		]
	},
	"activationEvents": [
		"*"
	],
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}